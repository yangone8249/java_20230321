package day16;

import java.util.ArrayList;
import java.util.Scanner;

public class Product {

	
	private String menu;
	private int price;
	
	
	
	
	public String getMenu() {
		return menu;
	}
	public void setMenu(String menu) {
		this.menu = menu;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	
	
	
}
