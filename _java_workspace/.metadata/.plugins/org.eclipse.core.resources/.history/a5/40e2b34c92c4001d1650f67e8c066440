package day18;

import java.util.Scanner;

public class test {
	public static void main(String[]args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.println("거스름돈 입력");
		int money = scan.nextInt();
		int a=5000,b=1000,c=500,d=100;
		money %=a;
		System.out.printf("5000원권:%d장 ",money/a);
		money %=b;
		System.out.printf("1000원권:%d장 ",money/b);
		money %=c;
		System.out.printf("500원권:%d장 ",money/c);
		money %=d;
		System.out.printf("100원권:%d장 ",money/d);
		
		
	}
}
